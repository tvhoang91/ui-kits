{
  "name": "@ui-kits/registry",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "build:default": "shadcn build --config registry-default.json --output public/r/default",
    "build:yay": "shadcn build --config registry-yay.json --output public/r/yay",
    "build:seed": "shadcn build --config registry-seed.json --output public/r/seed",
    "build:all": "pnpm build:default && pnpm build:yay && pnpm build:seed",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/postcss": "^4.1.11",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.536.0",
    "next": "^14.0.0",
    "postcss": "^8.5.6",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "shadcn": "latest",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5.0.0"
  }
}